{%- if vllm['pipeline-parallel-size'] is defined -%}{%- set _ = args.append('--pipeline-parallel-size ' ~ vllm['pipeline-parallel-size']) -%}{%- endif -%}
{%- if vllm['block-size'] is defined -%}{%- set _ = args.append('--block-size ' ~ vllm['block-size']) -%}{%- endif -%}
